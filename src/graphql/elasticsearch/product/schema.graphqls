scalar Date

type Product {
  id: Int!
  sku: String
  name: String
  status: Int
  visibility: Int
  type_id: String
  created_at: Date
  updated_at: Date
  weight: Float
  description: String
  image: String
  small_image: String
  thumbnail: String
  price: Float
  max_price: Float
  minimal_price: Float
  regular_price: Float
  max_regular_price: Float
  minimal_regular_price: Float
  final_price: Float
}

type Query {
  Product(id: Int!): Product
  ProductList(query: String!): [Product]
}
